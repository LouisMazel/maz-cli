{
  "name": "maz-cli",
  "version": "0.0.14",
  "description": "CLI Tool to create Nuxt/Prismic project",
  "author": "Louis Mazel <me@loicmazuel.com>",
  "bin": {
    "maz": "bin/maz.js"
  },
  "scripts": {
    "lint": "eslint './bin' './lib'",
    "lint:fix": "npm run lint -- --fix",
    "serve": "node ./bin/maz.js",
    "pre-publish": "npm run lint && npm i"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "keywords": [
    "maz-cli"
  ],
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.0.0",
    "clear": "^0.1.0",
    "cross-spawn": "^7.0.2",
    "download-git-repo": "^3.0.2",
    "figlet": "^1.3.0",
    "fs-extra": "^9.0.0",
    "inquirer": "^7.1.0",
    "lodash": "^4.17.15",
    "minimist": "^1.2.5",
    "ora": "^4.0.3",
    "replace-in-file": "^5.0.2",
    "text-to-svg": "^3.1.5"
  },
  "devDependencies": {
    "commander": "^5.0.0",
    "eslint": "^6.8.0",
    "husky": "^4.2.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LouisMazel/maz-cli.git"
  },
  "bugs": {
    "url": "https://github.com/LouisMazel/maz-cli/issues"
  },
  "homepage": "https://github.com/LouisMazel/maz-cli"
